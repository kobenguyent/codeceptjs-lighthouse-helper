{
  "name": "codeceptjs-lighthouse-helper",
  "version": "1.0.0",
  "description": "Lighthouse Audit Helper for codeceptjs",
  "main": "src/index.ts",
  "scripts": {
    "lint": "rome check src",
    "lint:fix": "rome check src --apply-suggested",
    "prepare": "husky install",
    "code:format": "rome format src --write",
    "tsc": "tsc"
  },
  "keywords": [
    "codeceptjs",
    "lighthouse-playwright"
  ],
  "license": "ISC",
  "dependencies": {
    "@codeceptjs/helper": "^2.0.1",
    "lighthouse": "^9.6.8",
    "playwright-lighthouse": "^2.2.2",
    "codeceptjs": "^3.4.1"
  },
  "devDependencies": {
    "flowmatic-cicd": "^1.2.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "typescript": "^4.9.5",
    "rome": "^11.0.0"
  },
  "files": [
    "src/**",
    "README.md",
    "bin/parallel.js"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kobenguyent/codeceptjs-lighthouse-helper.git"
  },
  "bugs": {
    "url": "https://github.com/kobenguyent/codeceptjs-lighthouse-helper/issues"
  },
  "homepage": "https://github.com/kobenguyent/codeceptjs-lighthouse-helper#readme",
  "author": "Thanh Nguyen <kobenguyent@gmail.com>",
  "lint-staged": {
    "src/*": "rome check src"
  }
}
